	
		<% if @video_skater.video_skater_url.include? "youtube.com"
        @numero = @video_skater.video_skater_url.partition("=").last
        @video_code = '<iframe width=100% height=400px src="//www.youtube.com/embed/'+@numero+'" frameborder="0" allowfullscreen></iframe>'
        end %>
        <% if @video_skater.video_skater_url.include? "vimeo.com" 
         @numero = @video_skater.video_skater_url.partition(".com/").last
         @video_code = '<iframe width=100% height=400px  src="//player.vimeo.com/video/'+@numero+'" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>'
        end %>


<p>
 <div class = "secciones" style = "font-size:200%"><%= @video_skater.video_skater_title.upcase %></div> 
</p>

<p>
   <% if current_user.try(:admin?) %><p></p>
   | <%= link_to 'Editar', (edit_video_skater_path(:id => @video_skater.id, :skater_id => @video_skater.skater_id, :skater_name => @video_skater.skater.skater_name)) %> |
   <%= link_to 'Eliminar', @video_skater, method: :delete, data: { confirm: 'Esta seguro que desea eliminar la foto?' } %> |
   <% end %> 
</p>

<p>
 <div class="fuente-tres"><div style = "color:#866A6A;">SKATER : <%= @video_skater.skater.skater_name.upcase %></div></div>
</p>

<p>
<div><%= @video_code.html_safe %></div>
</p>



