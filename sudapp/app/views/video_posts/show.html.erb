	
		<% if @video_post.video_post_url.include? "youtube.com"
        @numero = @video_post.video_post_url.partition("=").last
        @video_code = '<iframe width=100% height=400px src="//www.youtube.com/embed/'+@numero+'" frameborder="0" allowfullscreen></iframe>'
        end %>
        <% if @video_post.video_post_url.include? "vimeo.com" 
         @numero = @video_post.video_post_url.partition(".com/").last
         @video_code = '<iframe width=100% height=400px  src="//player.vimeo.com/video/'+@numero+'" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>'
        end %>




<p>
 <div class = "secciones" style = "font-size:200%"><%= @video_post.video_post_title.upcase %></div> 
</p>

<p>
   <% if current_user.try(:admin?) %><p></p>
   | <%= link_to 'Editar', (edit_video_post_path(:id => @video_post.id, :post_id => @video_post.post_id, :post_title => @video_post.post.post_title)) %> |
   <%= link_to 'Eliminar', @video_post, method: :delete, data: { confirm: 'Esta seguro que desea eliminar la foto?' } %> |
   <% end %> 
</p>

<p>
 <div class="fuente-tres"><div style = "color:#866A6A;">NOTA : <%= @video_post.post.post_title.upcase %></div></div>
</p>

<p>
<div><%= @video_code.html_safe %></div>
</p>



